<div class="transitions-enabled" id="pins-wrapper">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag pin.image.url(:medium)),pin %>
      <p>
        <%= pin.description %>
      </p>
      <div class="user">
        <p class="date"><%= pin.created_at.strftime("%B, %d, %Y") %>
        <span class="glyphicon glyphicon-heart"></span></p>
      </div>
    </div>
  <% end %>
</div>
